<mujoco model="Basic arm scene">
    <include file="assets/basic_scene.xml"/>
    <include file="assets/assets.xml"/>
<!--    <include file="assets/gripper_assets.xml"/>-->
    <compiler meshdir=""/>

    <option>
        <flag multiccd="enable"/>
    </option>

    <default>
        <default class="obstacle_geom">
            <geom solref="0.05 0.7" solimp="0.7 0.95 0.01 0.5 2" friction="1 0.005 0.0001" condim="3" margin="0.001"/>
        </default>
    </default>

    <worldbody>
        <!-- Include the franka_panda robot arm model -->
        <include file="assets/panda_with_pump.xml"/>`

        <body pos="0.8 0.8 0.03" name="Tomato_passata">
            <joint type="free" />
            <!--<site name="b1" pos="0 0 -0.031" quat = "0 0 0 1" rgba="1 0 0 1"/>-->
            <geom type = "box" pos = "0 0 0" size = "0.045 0.0475 0.03" rgba = "0 0.9 0.1 1" mass="0.5" friction = "1 0.5 0.1" condim="3" margin = "0.001" solimp="0.4 0.9 0.001 0.5 2" solref = "0.06 1.0"/>
        </body>

        <!-- Purple big cylinder -->
        <body pos="0 -0.27 0.07" name="Hot_chocolate">
            <joint type="free" />
            <geom type = "cylinder" class="obstacle_geom" pos = "0 0 0" size = "0.045 0.07" rgba = "1 0 1 1" mass="0.574"/>
        </body>

        <!-- Red small cylinder -->
        <body pos="0.42 0.4 0.05" name="Tomato_soup">
            <joint type="free" />
            <geom type = "cylinder" class="obstacle_geom" pos = "0 0 0" size = "0.035 0.05" rgba = "1 0 0 1" mass="0.305"/>
        </body>

        <!-- Turqoise box-->
        <body pos="1.0 -0.35 0.03" name="Decaf_tea_box">
            <joint type="free" />
            <geom type = "box" class="obstacle_geom" pos = "0 0 0" size = "0.105 0.10 0.155" rgba = "0 0.5 0.5 1" mass="0.31"/>
        </body>

        <!-- Yellow object -->
        <body pos="1 -0.25 0.06" name="Napolina_pasta">
            <joint type="free" />
            <geom type = "box" class="obstacle_geom" pos = "0 0 0" size = "0.045 0.04 0.06" rgba = "0.4 0.5 0.2 1" mass="0.6" />
        </body>

        <!-- Medium Gray cylinder-->
        <body pos= "-1 -0.35 0.02" name="Napolina_passata">
            <joint type="free" />
            <geom type = "cylinder" class="obstacle_geom" pos = "0 0 0" size = "0.06 0.05 0.02" rgba = "0.2 0.2 0.2 1" mass="0.4"/>
        </body>

        <!-- Dark blue box -->
        <body pos="0.6 1 0.035" name="Nature_valley_box">
            <joint type="free" />
            <geom type = "box" class="obstacle_geom" pos = "0 0 0" size = "0.06 0.04 0.035" rgba = "0 0 1 1" mass="0.6"/>
        </body>

        <!-- black box -->
        <body pos="1 1 0.035" name="Bisto_red_1">
            <joint type="free" />
            <geom type = "box" class="obstacle_geom" pos = "0 0 0" size = "0.03 0.04 0.035" rgba = "0.4 0 0.6 1" mass="0.6"/>
        </body>

        <!-- Top wall -->
        <geom name="top_wall" type="box" pos="0.6 -0.3 0.05" size="0.01 0.15 0.05" rgba="0.4 0.4 0.4 1"/>
        <!-- Right wall -->
        <geom name="right_wall" type="box" pos="0.35 -0.45 0.05" size="0.25 0.01 0.05" rgba="0.4 0.4 0.4 1"/>
        <!-- Bottom wall -->
        <geom name="bottom_wall" type="box" pos="0.1 -0.3 0.05" size="0.01 0.15 0.05" rgba="0.4 0.4 0.4 1"/>
        <!-- Left wall -->
        <geom name="left_wall" type="box" pos="0.35 -0.15 0.05" size="0.25 0.01 0.05" rgba="0.4 0.4 0.4 1"/>

        <body pos="0.5 0.1 0" name="target" gravcomp="1">
            <joint type="free" />
            <site name="display" pos="0 0 0" rgba="1 0 0 1"/>
            <geom type = "box" pos = "0 0 0" size = "0.045 0.0475 0.03" rgba = "0 1 0 0.4" mass="1" margin = "0.001" contype = "8" conaffinity = "8" />
        </body>

    </worldbody>

    <include file='assets/actuator_torques.xml'/>

<!--    <equality>-->
<!--        <weld name="pump weld" site1="force_sensor" site2="b1" solimp="0.4 0.99 0.001 0.5 2" solref = "0.02 1"/>-->
<!--    </equality>-->

</mujoco>
