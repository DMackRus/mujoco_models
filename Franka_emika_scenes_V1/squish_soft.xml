    <include file="assets/basic_scene.xml"/>
    <include file="assets/assets.xml"/>
    <include file="assets/gripper_assets.xml"/>
    <compiler meshdir=""/>

    <extension>
        <plugin plugin="mujoco.elasticity.solid"/>
    </extension>

    <default>
        <default class="wall">
            <geom type="plane" size=".5 .5 .05"/>
        </default>
    </default>

    <size memory="100M"/>

    <worldbody>

        <!-- Include the franka_panda robot arm model -->
        <include file="assets/chain0.xml"/>
        <body pos="0.7 0.02 0.041" name="goal">
            <joint type="free" />
            <geom type = "cylinder" pos = "0 0 0" size = "0.06 0.08" mass="0.74" rgba = "0 1 0 1" solimp="0.4 0.99 0.01 0.5 2" solref = "0.1 0.8" condim="3" contype = "3" conaffinity = "3"  margin = "0.001"/>
        </body>

        <body pos="1 0 0" name="display_goal">
            <joint type="free" />
            <geom type = "box" pos = "0 0 0" size = ".0001 .0001 .0000001" rgba = "0 0 0 0" mass="1" margin = "0.001" contype = "4" conaffinity = "4" />
            <site type="cylinder" pos="0 0 0.085" size="0.06 0.085" rgba="0 1 0 0.4"/>
        </body>

<!--        <body mocap="true" pos="-.1 .05 0" zaxis=".5 0 1">-->
<!--            <geom type="capsule" size=".1 .1" group="1" condim="1"/>-->
<!--        </body>-->
        <!--        <geom name="+x" class="wall" zaxis="1 0 0"  pos="-.5 0 -.25"/>-->
        <geom name="-x" class="wall" zaxis="-1 0 0" pos=".7 0 -.25"/>
        <!--        <geom name="+y" class="wall" zaxis="0 1 0"  pos="0 -.5 -.25"/>-->
        <!--        <geom name="-y" class="wall" zaxis="0 -1 0" pos="0 .5 -.25"/>-->

        <flexcomp name="jelly" type="grid" count="3 4 3" spacing=".07 .07 .07" pos="0.6 0 0.1" dim="3" radius=".005" rgba="1 0 0 1" mass="1" >
            <edge damping="1"/>
            <contact selfcollide="none"/>
            <plugin plugin="mujoco.elasticity.solid">
                <config key="poisson" value="0"/>
                <!--1-Units are in Pa (SI) -->
                <config key="young" value="3e3"/>
            </plugin>
        </flexcomp>

    </worldbody>

    <include file='assets/actuator0.xml'/>
</mujoco>